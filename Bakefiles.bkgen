<?xml version="1.0" ?>
<bakefile-gen xmlns="http://www.bakefile.org/schema/bakefile-gen">
    <input>
        ./mysql++.bkl
    </input>

    <!-- List of output formats to generate: -->
    <add-formats>
        autoconf,mingw,msvs2003prj,xcode2
    </add-formats>

    <add-flags formats="mingw">
        -o$(INPUT_FILE_DIR)/Makefile.mingw
    </add-flags>
</bakefile-gen>
